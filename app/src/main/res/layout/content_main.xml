        <com.github.mikephil.charting.charts.LineChart
            android:id="@+id/myChart8"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginBottom="20dp"
            >
        </com.github.mikephil.charting.charts.LineChart>

        <com.github.mikephil.charting.charts.LineChart
            android:id="@+id/myChart9"
            android:layout_width="match_parent"
